<Window x:Class="osuSongPackager.ParseSongs"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:osuSongPackager"
        mc:Ignorable="d"
        Title="Parse Songs" Height="150" Width="400" MinHeight="150" MinWidth="400"
        WindowStyle="None" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40" />
            <RowDefinition Height="40" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        
        <Label Content="Label" HorizontalAlignment="Stretch" Margin="5,5,5,5" VerticalAlignment="Top" Grid.Row="0"/>
        <Label Content="Label" HorizontalAlignment="Stretch" Margin="5,5,5,5" VerticalAlignment="Top" Grid.Row="1"/>
        <ProgressBar Height="35" Margin="10,0,10,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"
                             Grid.Row="2"/>
    </Grid>
</Window>
